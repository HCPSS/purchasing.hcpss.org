{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
      "composer": {
          "type": "composer",
          "url": "https://packages.drupal.org/8"
      },
      "parity": {
          "type": "package",
          "package": {
              "name": "hcpss/parity",
              "version": "dev-master",
              "type": "drupal-theme",
              "source": {
                  "url": "https://github.com/HCPSS/parity.git",
                  "type": "git",
                  "reference": "master"
              }
          }
      },
      "d3": {
          "type": "package",
          "package": {
              "name": "d3/d3",
              "version": "v3.5.17",
              "type": "drupal-library",
              "dist": {
                  "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
                  "type": "zip"
              }
          }
      },
      "highlightjs": {
          "type": "package",
          "package": {
              "name": "components/highlightjs",
              "version": "9.7.0",
              "type": "drupal-library",
              "dist": {
                  "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
                  "type": "zip"
              }
          }
      }
  },
    "require": {
        "components/highlightjs": "^9.7",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "d3/d3": "^3.5",
        "drupal/addanother": "^1.1",
        "drupal/address": "^1.9",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/date_range_formatter": "9.0.x-dev@dev",
        "drupal/drd_agent": "^4.0",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/editor_file": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fixed_text_link_formatter": "^1.1@dev",
        "drupal/google_analytics": "^3.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/node_title_validation": "^1.22",
        "drupal/page_manager": "^4.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/search_api": "^1.19",
        "drupal/search_api_solr": "^4.1",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/token": "^1.9",
        "drupal/views_data_export": "^1.0",
        "drush/drush": "^10.4",
        "hcpss/parity": "dev-master",
        "phpoffice/phpspreadsheet": "^1.16",
        "simplesamlphp/simplesamlphp": "^1.19"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
          "drupal/core": {
            "Issue 3056234": "https://www.drupal.org/files/issues/2019-10-23/3056234-10.patch"
          }
        }
    },
    "require-dev": {
        "drupal/devel": "^4.1",
        "drupal/vardumper": "^1.5"
    }
}
