{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "composer": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "parity": {
            "type": "package",
            "package": {
                "name": "hcpss/parity",
                "version": "dev-master",
                "type": "drupal-theme",
                "source": {
                    "url": "https://github.com/HCPSS/parity.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "d3": {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
                    "type": "zip"
                }
            }
        },
        "highlightjs": {
            "type": "package",
            "package": {
                "name": "components/highlightjs",
                "version": "9.7.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "components/highlightjs": "9.7.*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "d3/d3": "3.5.*",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/addanother": "^1.0@RC",
        "drupal/address": "^1.3",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/config_installer": "^1.8",
        "drupal/config_split": "^1.3",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.2",
        "drupal/date_range_formatter": "^3.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.2",
        "drupal/events_log_track": "^1.1",
        "drupal/field_group": "^3.0@beta",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fixed_text_link_formatter": "^1.1",
        "drupal/google_analytics": "^2.3",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/node_title_validation": "^1.1",
        "drupal/page_manager": "^4.0@beta",
        "drupal/paragraphs": "^1.6",
        "drupal/pathauto": "^1.2",
        "drupal/search_api": "^1.8",
        "drupal/search_api_solr": "^4.0",
        "drupal/simplesamlphp_auth": "^3.0",
        "drupal/token": "^1.1",
        "drupal/views_data_export": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "egulias/email-validator": "^2.1",
        "hcpss/parity": "dev-master",
        "phpoffice/phpspreadsheet": "^1.4",
        "simplesamlphp/simplesamlphp": "^1.18",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "^2.0",
        "drupal/vardumper": "^1.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        	"drupal/core": {
        		"Issue 3056234": "https://www.drupal.org/files/issues/2019-05-22/3056234-test-fix-4.patch"
          },
          "drupal/search_api": {
            "Issue 790418": "https://www.drupal.org/files/issues/2020-10-26/search_api-use-entity-type-manager-interface-790418-1.patch"
          }
        }
    }
}
